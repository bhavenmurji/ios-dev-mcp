{
  "name": "ios-dev-mcp",
  "version": "0.1.0",
  "description": "Unified MCP server for iOS development - Swift execution, Xcode building, simulator control, and testing",
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node build/index.js",
    "dev": "tsc --watch & node --watch build/index.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "ios",
    "xcode",
    "swift",
    "simulator",
    "testing",
    "development",
    "claude",
    "ai"
  ],
  "author": "Bhaven Murji",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bhavenmurji/ios-dev-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/bhavenmurji/ios-dev-mcp/issues"
  },
  "homepage": "https://github.com/bhavenmurji/ios-dev-mcp#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
